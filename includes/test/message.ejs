<div class="mb-message <%= (data.images && data.images.length > 0) ? 'has-image' : '' %> <%= (data.webpages && data.webpages.length > 0) ? 'has-site' : '' %>" data-message-id="<%= data.id %>">
  <a class="mb-unfocus">X</a>
  <div class="header">
    <h2 class="author">
      <%= data.author %>
      <span class="dt-posted"><%= data.dt_posted %></span>
    </h2>
  </div>
  <div class="content">
    <%= data.content %>
  </div>
  <% if(data.images && data.images.length > 0){ %>
    <img class="mb-image main-image" src="<%= data.images[0].url %>"></img>
  <% } %>
  <% if(data.webpages && data.webpages.length > 0){ %>
    <div class="mb-site-content site-content"></div>
  <% } %>
</div>