<div class="mb-message" data-message-id="<%= data.id %>">
  <a class="mb-unfocus">X</a>
  <div class="top-bar">
    <% if(data.webpages && data.webpages.length > 0){ %>
      <%= data.webpages[0].title %>
    <% } %>
  </div>
  <div class="header">
    <img src="http://tuxion.nl/wip/social/11/files/images/icons/twitter.png" />
    Geplaatst door <span class="author"><a href="https://twitter.com/<%= data.author %>" target="_blank"><%= data.author %></a></span>
    Op <span class="dt-posted"><a href="https://twitter.com/bumaxNL/status/327532449691209728" target="_blank"><%= data.dt_posted %></a></span>
    </h2>
  </div>
  <div class="content">
    <%= data.content %>
  </div>
  <% if(data.webpages && data.webpages.length > 0){ %>
    <div class="mb-site-content site-content"></div>
  <% } %>
</div>